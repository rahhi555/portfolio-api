response = nil

case @column
when 'images'
  response = @todo.image_urls(1000, 560)
else
  response = {
               id: @todo.id,
               todoListId: @todo.todo_list_id,
               title: @todo.title,
               body: @todo.body,
               beginTime: @todo.begin_time,
               endTime: @todo.end_time,
               status: @todo.status,
               images: @todo.image_urls
             }
end

response