todos = @todo_list.todos.map do |todo|
  {
    id: todo.id,
    todoListId: todo.todo_list_id,
    title: todo.title,
    body: todo.body,
    beginTime: todo.begin_time,
    endTime: todo.end_time,
    status: todo.status,
    images: todo.image_urls
  }
end

{
  id: @todo_list.id,
  planId: @todo_list.plan_id,
  title: @todo_list.title,
  todos: todos
}
